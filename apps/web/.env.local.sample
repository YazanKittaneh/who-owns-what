# Public Convex deployment URL used by browser and server reads.
NEXT_PUBLIC_CONVEX_URL=

# Optional server-side override (usually same as NEXT_PUBLIC_CONVEX_URL).
CONVEX_URL=

# Required for import/deploy scripts that mutate Convex data.
CONVEX_DEPLOY_KEY=

# Optional local fallback flag.
# Keep unset in production. For local smoke tests only.
WOW_ALLOW_SAMPLE_DATA=
